{
	"name": "dragonscript-language-support",
	"displayName": "DragonScript Language Support",
	"description": "",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "dragonscript",
				"aliases": [
					"DragonScript",
					"dragonscript"
				],
				"extensions": [
					".ds"
				],
				"configuration": "./language-configuration.json"
			}
