{
	"name": "dragonscript-language-support",
	"displayName": "DragonScript Language Support",
	"publisher": "dragondreams",
	"description": "Provides Syntax Hilighting support for DragonScript Script Language files (*.ds).",
	"license": "SEE LICENSE IN License",
	"repository": {
		"type": "git",
		"url": "https://github.com/LordOfDragons/vscode-langext-dragonscript"
	},
	"galleryBanner": {
		"color": "#7dafff",
		"theme": "dark"
	},
	"icon": "images/icon.png",
	"keywords": [
		"dragonscript",
		"dragengine"
	],
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [{
			"id": "dragonscript",
			"aliases": ["DragonScript", "dragonscript"],
			"extensions": [".ds"],
			"configuration": "./language-configuration.json"
		}],
		"grammars": [{
			"language": "dragonscript",
			"scopeName": "source.dragonscript",
			"path": "./syntaxes/dragonscript.tmLanguage.json"
		}]
	}
}
